{
    "cniVersion": "0.3.1",
    "name": "bridge",
    "type": "bridge",
    "bridge": "cnio0",
    "isGateway": true,
    "ipMasq": true,
    "ipam": {
        "type": "host-local",
        "ranges": [
          [{"subnet": "{{  cluster_pod_cidr | ipsubnet(24, groups['all'].index(inventory_hostname)) }}"}]
        ],
        "routes": [{"dst": "0.0.0.0/0"}]
    }
}
